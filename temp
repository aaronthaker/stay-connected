import json

def describe_structure(data, indent=0):
    prefix = ' ' * indent
    if isinstance(data, dict):
        print(f"{prefix}Object with {len(data)} keys:")
        for key, value in data.items():
            print(f"{prefix}- Key: '{key}'")
            describe_structure(value, indent + 4)
    elif isinstance(data, list):
        print(f"{prefix}List with {len(data)} elements.")
        if data:
            print(f"{prefix}First element structure:")
            describe_structure(data[0], indent + 4)
    else:
        print(f"{prefix}Type: {type(data).__name__} | Example: {repr(data)}")

def main():
    print("Paste your JSON object below (single line or multiline), then press Enter twice:")
    input_lines = []
    while True:
        try:
            line = input()
            if line == "":
                break
            input_lines.append(line)
        except EOFError:
            break

    json_str = '\n'.join(input_lines)
    try:
        json_obj = json.loads(json_str)
        print("\nStructure of your JSON object:")
        describe_structure(json_obj)
    except json.JSONDecodeError as e:
        print(f"Invalid JSON input: {e}")

if __name__ == "__main__":
    main()