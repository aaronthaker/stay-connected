import json

def print_json_structure(data, indent=0, max_items=3):
    spacing = "  " * indent
    if isinstance(data, dict):
        for key, value in data.items():
            print(f"{spacing}- {key}:")
            print_json_structure(value, indent + 1, max_items)
    elif isinstance(data, list):
        print(f"{spacing}- [List] ({len(data)} items)")
        for i, item in enumerate(data[:max_items]):
            print(f"{spacing}  [{i}]")
            print_json_structure(item, indent + 2, max_items)
        if len(data) > max_items:
            print(f"{spacing}  ...and {len(data) - max_items} more items")
    else:
        # Print a sample value
        print(f"{spacing}- {repr(data)} ({type(data).__name__})")

# Load and print structure
with open("test.json", "r", encoding="utf-8") as f:
    json_data = json.load(f)

print("JSON Structure:")
print_json_structure(json_data)